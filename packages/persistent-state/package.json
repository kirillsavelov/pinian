{
  "name": "@pinian/persistent-state",
  "version": "0.1.0",
  "description": "This plugin provides effortless state persistence for Pinia stores with a flexible, easy-to-use API. From simple defaults to advanced customizations like multi-storage and serializers, it streamlines state management, making it a perfect fit for any project.",
  "keywords": ["vue", "pinia", "pinia-plugin", "store", "persistence"],
  "homepage": "https://github.com/kirillsavelov/pinian",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirillsavelov/pinian.git"
  },
  "bugs": {
    "url": "https://github.com/kirillsavelov/pinian/issues"
  },
  "license": "MIT",
  "author": "Kirill Savelov",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      },
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "files": ["dist", "LICENSE", "README.md"],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.11",
    "@rslib/core": "^0.0.13",
    "pinia": "^2.2.4",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "pinia": "^2.0.0"
  }
}
